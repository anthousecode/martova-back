    "paths": {
        "/search-area/{num}": {
            "get": {
                "tags": [
                    "All"
                ],
                "summary": "Search for specific area",
                "operationId": "searchArea",
                "parameters": [
                    {
                        "name": "num",
                        "in": "path",
                        "description": "Area cadastral number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "property": "areas_ids",
                                    "description": "Areas with equal cadastral number, as passed",
                                    "type": "array",
                                    "items": {}
                                }
                            }
                        }
                    }
                }
            }
        },
